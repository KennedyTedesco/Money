{
  "name": "kennedytedesco/money",
  "description": "A simple library to handle basic operations with money.",
  "keywords": ["money", "currency", "php"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Kennedy Tedesco",
      "email": "kennedyt.tw@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "KennedyTedesco\\Money\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
